{
    "manifest_version": 2,
    "name": "Gmail Quick Open",
    "version": "1.0",
    "author": "Laurent Depoorter",
    "description": "Open Gmail's email in a new window",
    "permissions": [
        "*://mail.google.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://mail.google.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "icons/ico-16.png",
        "48": "icons/ico-48.png",
        "128": "icons/ico-128.png"
    },
    "default_locale": "en"
}